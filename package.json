{
  "name": "kiwi-traffic",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "bundle-android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle",
    "bundle-android-old": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "bundle-ios": "react-native bundle --dev false --entry-file index.js --bundle-output ios/main.jsbundle --platform ios",
    "debugger-open": "rndebugger-open --port 8081",
    "watchman-clear": "watchman watch-del-all && rm -rf node_modules/ && yarn cache clean && yarn install",
    "install-apk": "adb install android/app/release/app-release.apk",
    "uninstall-apk": "adb uninstall com.kiwitraffic",
    "reinstall-apk": "npm run uninstall-apk && npm run install-apk",
    "build-ios": "cd ios && xcodebuild -workspace kiwiTraffic.xcworkspace -scheme kiwiTraffic  -configuration Release SYMROOT=/Builds/ios/kiwiTraffic"
  },
  "dependencies": {
    "@eva-design/eva": "^1.0.1",
    "axios": "^0.19.0",
    "ramda": "^0.26.1",
    "react": "16.8.6",
    "react-native": "0.60.4",
    "react-native-dotenv": "^0.2.0",
    "react-native-fast-image": "^7.0.2",
    "react-native-gesture-handler": "^1.3.0",
    "react-native-modal": "^11.1.0",
    "react-native-ui-kitten": "^4.0.5",
    "react-native-xml2js": "^1.0.3",
    "react-navigation": "^3.11.0",
    "react-redux": "^7.1.0",
    "redux": "^4.0.1",
    "redux-actions": "^2.6.5",
    "redux-thunk": "^2.3.0",
    "styled-components": "^4.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-transform-regenerator": "7.4.4",
    "@babel/runtime": "^7.4.5",
    "babel-jest": "^24.8.0",
    "babel-plugin-jsx-control-statements": "^4.0.0",
    "jest": "^24.8.0",
    "jetifier": "^1.6.3",
    "metro-react-native-babel-preset": "^0.54.1",
    "react-native-debugger-open": "^0.3.19",
    "react-test-renderer": "16.8.3"
  },
  "jest": {
    "preset": "react-native"
  }
}
